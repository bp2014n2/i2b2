<?xml version="1.0" encoding="UTF-8"?>
<ont:examples xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ont="http://www.i2b2.org/xsd/cell/ont/"
    xsi:schemaLocation="http://www.i2b2.org/xsd/cell/ont/ ./ONT_VMO_QRY.xsd">  
  
    <!-- REQUEST METADATA TABLE NAMES -->
    <message_body>
        <ont:request echo="true" sql="true">
            <vocab_meta>
                <return_list>
                    <return item="tablename"/>
                    <return item="tablesize"/>
                </return_list>
            </vocab_meta>
        </ont:request>
        <ont:response>
            <vocab_meta>
                <table_name>i2b2</table_name>
                <table_size>500</table_size>
            </vocab_meta>
            <vocab_meta>
                <table_name>testrpdr</table_name>
                <table_size>200000</table_size>
            </vocab_meta>
        </ont:response>
    </message_body>
 
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
    
